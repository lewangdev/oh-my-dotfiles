[core]
    editor = vim
    autocrlf = input
    #safecrlf = true
    whitespace = cr-at-eol

[color]
    ui = auto
[user]
    name = 小码哥
    email = thisiswangle@gmail.com
[merge]
    tool = vimdiff
[alias]
    st = status
    co = checkout
    ci = commit
    br = branch
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[http]
    #sslVerify = false
[credential]
	helper = store
